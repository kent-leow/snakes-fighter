{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "games": {
      "$gameId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "players": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "lobbies": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}